{
  "name": "ccmiller2018/cakephp-soft-delete",
  "description": "SoftDelete plugin for CakePHP",
  "license": "MIT",
  "require": {
    "php": ">=7.2",
    "cakephp/plugin-installer": "*",
    "cakephp/cakephp": "~4.2.10"
  },
  "require-dev": {
    "phpunit/phpunit": "*",
    "ext-mbstring": "*"
  },
  "autoload": {
    "psr-4": {
      "SoftDelete\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SoftDelete\\Test\\": "tests",
      "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
    }
  }
}